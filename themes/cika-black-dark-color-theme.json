{
  "name": "Cika",
  "type": "dark",
  "colors": {
    //
    "foreground": "#aaa",
    "editor.foreground": "#ddd",
    "editorLineNumber.activeForeground": "#aaa",
    "progressBar.background": "#aaa",
    "selection.background": "#aaa",
    "tab.activeBorder": "#aaa",
    "activityBar.foreground": "#aaa",
    "panelTitle.activeBorder": "#aaa",
    "panelTitle.activeForeground": "#aaa",
    //
    "panelTitle.inactiveForeground": "#333",
    "editor.lineHighlightBackground": "#333",
    "editorGroup.border": "#333",
    "sideBarSectionHeader.background": "#333",
    "editorLineNumber.foreground": "#333",
    "scrollbarSlider.background": "#333",
    "scrollbarSlider.hoverBackground": "#333",
    "scrollbarSlider.activeBackground": "#333",
    "list.activeSelectionBackground": "#333",
    "activityBar.inactiveForeground": "#333",
    "statusBar.foreground": "#333",
    "editorInlayHint.background": "#333",
    //
    "sideBar.border": "#111",
    "focusBorder": "#111",
    "dropdown.border": "#111",
    "list.inactiveSelectionBackground": "#111",
    "list.hoverBackground": "#111",
    "editorHoverWidget.background": "#111",
    "editorWidget.background": "#111",
    "editorSuggestWidget.background": "#111",
    "editorSuggestWidget.border": "#111",
    "editorHoverWidget.border": "#111",
    "activityBarBadge.background": "#111",
    "badge.background": "#111",
    "panel.border": "#111",
    "editorIndentGuide.background": "#111",
    //
    "editorIndentGuide.activeBackground": "#000",
    "editor.background": "#000",
    "peekViewEditor.background": "#000",
    "editorGroupHeader.tabsBackground": "#000",
    "titleBar.activeBackground": "#000",
    "titleBar.inactiveBackground": "#000",
    "tab.inactiveBackground": "#000",
    "tab.border": "#000",
    "activityBar.background": "#000",
    "sideBar.background": "#000",
    "statusBar.background": "#000",
    "statusBar.noFolderBackground": "#000",
    "dropdown.background": "#000",
    "input.background": "#000",
    "tab.activeBackground": "#000",
    //
  },
  "tokenColors": [
    {
      "scope": [
        "comment",
        "string.comment",
        "punctuation.definition.comment",
        "string.quoted.docstring.multi",
        "string.quoted.docstring.multi storage.type.string",
      ],
      "settings": {
        "foreground": "#5c6370",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "string",
        "punctuation.definition.template-expression.begin.js",
        "punctuation.definition.template-expression.end.js",
        "punctuation.definition.template-expression.begin.ts",
        "punctuation.definition.template-expression.end.ts",
        "string.template.js",
        "meta.template.expression.js",
        "string.embedded",
      ],
      "settings": {
        "foreground": "#98c379"
      }
    },
    {
      "name": "Number",
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#c678dd"
      }
    },
    {
      "scope": [
        "constant.language",
        "constant.character",
        "constant.other",
        "variable.language",
        "keyword",
        "keyword.operator.logical",
        "keyword.operator.constructor",
        "storage",
      ],
      "settings": {
        "foreground": "#FFD14A"
      }
    },
    {
      "scope": [
        // "entity.name.class",
        // "entity.name.module",
        // "entity.name.type",
        "storage.identifier",
        "support.class",
        "entity.other.inherited-class",
        "variable.other.block",
        "variable.other.object",
        "storage.type",
        "variable.readwrite",
        "variable.readwrite.other.block"
      ],
      "settings": {
        "foreground": "#61afef"
      }
    },
    {
      "scope": [
        // "variable.other.property",
        // "variable.other.object",
      ],
      "settings": {
        "foreground": "#FF0000",
        "fontStyle": "underline"
      }
    },
    {
      "scope": [
        "entity.name.function",
        "entity.name.function-call",
        "support.function",
        "function.support.builtin",
        "function.support.core",
        "support.constant",
        "variable.other.property",
        "entity.name.class",
        "entity.name.module",
        "entity.name.type",
        "support.type",
        "support.variable",
        "support.other.variable"
      ],
      "settings": {
        "foreground": "#00AEFF"
      }
    },
    {
      "scope": "variable.parameter",
      "settings": {
        "fontStyle": "italic",
        // "foreground": "#d19a66"
      }
    },
    // JSON ----------------------------------------------------------------------
    {
      "name": "[JSON] key",
      "scope": [
        "source.json",
        "support.dictionary.json",
        "support.type.property-name.json",
      ],
      "settings": {
        "foreground": "#ddd"
      }
    },
    {
      "name": "[JSON] value",
      "scope": [
        "source.json string",
        "source.json punctuation.definition.string",
        "punctuation.definition.string.end.json",
        "punctuation.definition.string.begin.json",
        "structure.dictionary.property-name.json"
      ],
      "settings": {
        "foreground": "#a8f"
      }
    },
    // HTML ----------------------------------------------------------------------
    {
      "name": "[HTML] - Entity",
      "scope": [
        "entity.other"
      ],
      "settings": {
        "foreground": "#8df"
      }
    },
    {
      "name": "[HTML] - Elements",
      "scope": [
        "entity.name.tag.block.any.html",
        "entity.name.tag.inline.any.html",
        "entity.name.tag.structure.any.html",
        "entity.name.tag.html",
        "meta.jsx.children.tsx"
      ],
      "settings": {
        "foreground": "#00AAEF"
      }
    },
    {
      "name": "[HTML] - Keywords",
      "scope": [
        "punctuation.definition.keyword",
        "keyword.control"
      ],
      "settings": {
        "foreground": "#FFD14A"
      }
    },
    {
      "name": "[HTML] - ID value",
      "scope": "meta.toc-list.id.html",
      "settings": {
        "foreground": "#91DD64"
      }
    },
    {
      "name": "[HTML] - Meta tags",
      "scope": [
        "meta.tag.block.any.html",
        "meta.tag.inline.any.html"
      ],
      "settings": {
        "foreground": "#00AEFF"
      }
    },
    {
      "name": "[HTML] - Tags",
      "scope": [
        "punctuation.definition.tag.end.html",
        "punctuation.definition.tag.begin.html"
      ],
      "settings": {
        "foreground": "#777"
      }
    },
    {
      "name": "[HTML] - Text basic",
      "scope": "text.html.basic",
      "settings": {
        "foreground": "#c3c3c3"
      }
    },
    {
      "name": "[HTML] - Quotes",
      "scope": "punctuation.definition.string.begin, punctuation.definition.string.end",
      "settings": {
        "foreground": "#98c379"
      }
    },
    // CSS ----------------------------------------------------------------------
    {
      "name": "[CSS] - Entity",
      "scope": [
        "source.css",
        "source.stylus",
        "source.scss",
        "entity.other.attribute-name.class.css"
      ],
      "settings": {
        "foreground": "#00AEFF"
      }
    },
    {
      "name": "[CSS] - Hex hex",
      "scope": [
        "punctuation.definition.constant.css"
      ],
      "settings": {
        "foreground": "#a8f"
      }
    },
    {
      "name": "[CSS] - keys",
      "scope": [
        "support.type.property-name.css"
      ],
      "settings": {
        "foreground": "#ddd"
      }
    },
    {
      "name": "[CSS] - Keyword",
      "scope": [
        "source.css punctuation.definition.keyword",
        "source.css keyword.control"
      ],
      "settings": {
        "foreground": "#f6b"
      }
    },
    {
      "name": "[CSS] - Important",
      "scope": "keyword.other.important.scss",
      "settings": {
        "foreground": "#f6b"
      }
    },
    {
      "name": "[CSS] - Punctiation",
      "scope": [
        "punctuation.definition.entity.css"
      ],
      "settings": {
        "foreground": "#00AEFF"
      }
    },
    {
      "name": "[CSS] - ID hastags",
      "scope": [
        "entity.other.attribute-name.pseudo-element.css"
      ],
      "settings": {
        "foreground": "#00AEFF"
      }
    },
    {
      "name": "[CSS] - support misc function",
      "scope": [
        "support.function.misc.scss"
      ],
      "settings": {
        "foreground": "#a8f"
      }
    },
    {
      "name": "[CSS] - ID Selector",
      "scope": [
        "entity.other.attribute-name.id.css"
      ],
      "settings": {
        "foreground": "#8df"
      }
    },
    {
      "name": "[CSS] - Element Selector",
      "scope": "entity.name.tag.css",
      "settings": {
        "foreground": "#00AEFF"
      }
    },
    {
      "name": "[CSS] - Support",
      "scope": [
        "source.css support",
        "source.stylus support",
        "source.css support.constant"
      ],
      "settings": {
        "foreground": "#ddd"
      }
    },
    {
      "name": "[CSS] - Constant",
      "scope": [
        "source.stylus constant",
        "source.css constant"
      ],
      "settings": {
        "foreground": "#a8f"
      }
    },
    {
      "name": "[CSS] - support constant",
      "scope": [
        "support.constant.property-value.css"
      ],
      "settings": {
        "foreground": "#FFD14A"
      }
    },
    {
      "name": "[CSS] - units",
      "scope": [
        "keyword.other.unit.css",
        "keyword.other.unit.px.css",
        "keyword.other.unit.percentage.css",
        "constant.other.color.rgb-value.hex.css",
        "keyword.other.unit.ms.css",
        "keyword.other.unit.s.css",
        "keyword.other.unit.vh.css",
        "keyword.other.unit.vw.css",
        "keyword.other.unit.deg.css",
        "keyword.other.unit.fr.css"
      ],
      "settings": {
        "foreground": "#a8f"
      }
    },
    {
      "name": "[CSS] - String",
      "scope": [
        "source.css string",
        "source.css punctuation.definition.string",
        "source.stylus string",
        "source.stylus punctuation.definition.string"
      ],
      "settings": {
        "foreground": "#98c379"
      }
    },
    {
      "name": "[CSS] - Variable",
      "scope": [
        "source.css variable",
        "source.stylus variable"
      ],
      "settings": {
        "foreground": "#FFD14A"
      }
    },
    // ----------------------------------------------------------------------
  ]
}