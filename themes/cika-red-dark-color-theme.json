	"tokenColors": [
		{
			"settings": {
				"foreground": "#fff"
			}
		},
		{
			"name": "Comment",
			"scope": [
				"comment",
				"string.comment",
				"punctuation.definition.comment",
				"string.quoted.docstring.multi",
				"string.quoted.docstring.multi storage.type.string",
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#555"
			}
		},
		{
			"name": "String",
			"scope": "string",
			"settings": {
				"fontStyle": "",
				"foreground": "#9AA83A"
			}
		},
		{
			"name": "String Embedded Source",
			"scope": "string source",
			"settings": {
				"fontStyle": "",
				"foreground": "#D08442"
			}
		},
		{
			"name": "Number",
			"scope": "constant.numeric",
			"settings": {
				"fontStyle": "",
				"foreground": "#6089B4"
			}
		},
		{
			"name": "Built-in constant",
			"scope": "constant.language",
			"settings": {
				"fontStyle": "",
				"foreground": "#959700"
			}
		},
		{
			"name": "User-defined constant",
			"scope": "constant.character, constant.other",
			"settings": {
				"fontStyle": "",
				"foreground": "#959700",
			}
		},
		{
			"name": "Keyword",
			"scope": "keyword",
			"settings": {
				"fontStyle": "",
				"foreground": "#6089B4"
			}
		},
		{
			"name": "Support",
			"scope": "support",
			"settings": {
				"fontStyle": "",
				"foreground": "#C7444A"
			}
		},
		{
			"name": "Storage",
			"scope": "storage",
			"settings": {
				"fontStyle": "",
				"foreground": "#63007c"
			}
		},
		{
			"name": "Class name",
			"scope": "entity.name.class, entity.name.type",
			"settings": {
				"fontStyle": "",
				"foreground": "#ff66009b",
			}
		},
		{
			"name": "Inherited class",
			"scope": "entity.other.inherited-class",
			"settings": {
				"fontStyle": "",
				"foreground": "#c7444b9a"
			}
